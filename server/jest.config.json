{
	"testEnvironment": "node",
	"testMatch": [
		"**/*.test.js"
	],
	"modulePathIgnorePatterns": [
		"<rootDir>/node_modules/"
	],
	"coveragePathIgnorePatterns": [
		"<rootDir>/node_modules/"
	],
	"collectCoverageFrom": [
		"**/*.js"
	],
	"coverageDirectory": "coverage",
	"coverageReporters": [
		"text",
		"lcov"
	]
}